{
    "name": "zidbih/laravel-liveapi",
    "description": "Generate accurate OpenAPI specifications by observing real Laravel API traffic at runtime, with zero configuration and no annotations.",
    "keywords": [
        "laravel",
        "openapi",
        "swagger",
        "api-documentation",
        "api",
        "openapi-3",
        "runtime",
        "zero-config",
        "schema-inference",
        "developer-tools"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Zidbih\\LiveApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zidbih\\LiveApi\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "zidbih",
            "email": "medmahmoudhdaya@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "laravel/pint": "^1.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zidbih\\LiveApi\\LiveApiServiceProvider"
            ]
        }
    }
}